{
  "name": "@netist/ui",
  "version": "0.1.7",
  "author": "Ebubekir Yazgan <yazganebubekir4@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "bekiryazgann/netist-design-system.git",
    "directory": "/"
  },
  "main": "dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    }
  },
  "sideEffects": false,
  "files": [
    "dist/**",
    "styles.css"
  ],
  "scripts": {
    "build": "node_modules/.bin/yarn clean && yarn build:js:cjs && yarn build:js:esm",
    "build:js:cjs": "node_modules/.bin/tsc --project tsconfig.cjs.json && tsc-alias -p tsconfig.cjs.json",
    "build:js:esm": "node_modules/.bin/tsc --project tsconfig.esm.json && tsc-alias -p tsconfig.esm.json",
    "clean": "node_modules/.bin/rimraf dist",
    "test": "node_modules/.bin/vitest --run",
    "test:watch": "node_modules/.bin/vitest",
    "test:coverage": "node_modules/.bin/vitest --run --coverage",
    "lint": "node_modules/.bin/eslint \"**/*.ts*\"",
    "storybook": "node_modules/.bin/storybook dev -p 6006",
    "storybook:build": "node_modules/.bin/storybook build",
    "typecheck": "node_modules/.bin/tsc --noEmit"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@storybook/addon-essentials": "^8.5.0",
    "@storybook/addon-interactions": "^8.5.0",
    "@storybook/addon-links": "^8.5.0",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^8.5.0",
    "@storybook/react": "^8.5.0",
    "@storybook/react-vite": "^8.5.0",
    "@storybook/test": "^8.5.0",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jsdom": "^21.1.7",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^0.32.4",
    "autoprefixer": "^10.4.20",
    "chromatic": "^6.24.1",
    "eslint": "^7.32.0",
    "eslint-plugin-storybook": "^0.11.2",
    "jsdom": "^22.1.0",
    "postcss": "^8.5.1",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^5.0.10",
    "storybook": "^8.5.0",
    "tailwindcss": "^3.4.17",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.3",
    "vite": "^4.5.9",
    "vite-plugin-turbosnap": "^1.0.3",
    "vitest": "^0.32.4"
  },
  "dependencies": {
    "@medusajs/eslint-config-ui": "^0.0.1",
    "@medusajs/icons": "^1.2.2",
    "@medusajs/ui-preset": "^1.1.4",
